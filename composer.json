{
  "name": "mybizna/automigrator",
  "description": "Declare database migrations and factory definitions inside Laravel models.",
  "keywords": [
    "php",
    "laravel",
    "database",
    "migrations",
    "factory",
    "definitions",
    "models",
    "eloquent"
  ],
  "license": "MIT",
  "version": "2025.5",
  "require": {
    "php": ">=8.0",
    "doctrine/dbal": "^2.0|^3.0|^4.0",
    "laravel/framework": "^7.0|^8.0|^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Mybizna\\Automigrator\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mybizna\\Automigrator\\Providers\\AutomigratorServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0",
    "mockery/mockery": "^1.5",
    "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0|^11.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
