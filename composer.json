{
    "name": "legodion/lucid",
    "description": "Declare database migrations and factory definitions inside Laravel models.",
    "keywords": [
        "php",
        "laravel",
        "database",
        "migrations",
        "factory",
        "definitions",
        "models",
        "eloquent"
    ],
    "license": "MIT",
    "require": {
        "doctrine/dbal": "^2.0|^3.0",
        "laravel/framework": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Legodion\\Lucid\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Legodion\\Lucid\\Providers\\LucidServiceProvider"
            ]
        }
    }
}
